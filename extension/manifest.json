{
  "manifest_version": 3,
  "name": "Phishing Detector",
  "version": "1.0",
  "description": "Phishing detection using Random Forest, YOLOv8, and BERT models.",
  "permissions": [
    "activeTab", 
    "scripting"
  ],
  "host_permissions": [
    "https://phishing-attack-detection-api.onrender.com/*",
    "http://127.0.0.1:8000/*" 
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}